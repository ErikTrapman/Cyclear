{% extends '::admin.base_normal.twig' %}

{% block title %}Renner show{% endblock %}

{% block content %}

    <h3>Renner edit</h3>

    <form action="{{ path('admin_renner_edit', { 'id': entity.cqranking_id }) }}"
          method="post" >
        {{ form_widget(edit_form) }}
        <p>
            <button class="btn btn-default" type="submit">Edit</button>
        </p>
    </form>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_renner') }}">
                Back to the list
            </a>
        </li>
        <li>
            <form action="{{ path('admin_renner_delete', { 'id': entity.cqranking_id }) }}"
                  method="post">
                {{ form_widget(delete_form) }}
                <button class="btn btn-default" type="submit">Delete</button>
            </form>
        </li>
    </ul>

{% endblock %}