{% if 0 == uitslag.ploegPunten and uitslag.rennerPunten > 0 and uitslag.ploeg %}
    <s>{{ uitslag.rennerPunten }}</s>
{% else %}
    {% if uitslag.rennerPunten != uitslag.ploegPunten and uitslag.ploegPunten > 0 %}
        <s>{{ uitslag.rennerPunten }}</s> {{ uitslag.ploegPunten }}
    {% else %}
        {{ uitslag.rennerPunten }}
    {% endif %}
{% endif %}
