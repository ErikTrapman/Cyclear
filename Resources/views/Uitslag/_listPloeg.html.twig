{% block content %}
<table class="records_list table table-condensed ">
    <thead>
        <tr>
            <th>Datum</th>
            <th>Pos</th>
            <th>Renner</th>
            <th>Wedstrijd</th>
            <th>Punten</th>
        </tr>
    </thead>
    <tbody>
    {% for uitslag in uitslagen %}
        <tr>
            <td>{{ uitslag.wedstrijd.datum|date('d-m-Y') }}</td>
            <td>{{ uitslag.positie }}</td>
            <td>{{ uitslag.renner.naam }}</td>
            <td>
                {% include "CyclearGameBundle:Wedstrijd:_singleRow.html.twig" with {wedstrijd:uitslag.wedstrijd, seizoen:uitslag.wedstrijd.seizoen } %}
                {# <a href="{{ path('wedstrijd_show',{wedstrijd:uitslag.wedstrijd.id,seizoen:uitslag.wedstrijd.seizoen}) }}">{{ uitslag.wedstrijd.naam }}</a> #}
            </td>
            <td>{{ uitslag.ploegPunten }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
<div id="navigation">
    {{ knp_pagination_render(uitslagen) }}
</div>
{% endblock %}