{% block content %}
<table class="records_list table ">
    <thead>
        <tr>
            <th>Datum</th>
            <th>Wedstrijd</th>
            <th>Renner</th>
            <th>Ploeg</th>
            <th>Punten ploeg</th>
            <th>Punten renner</th>
        </tr>
    </thead>
    <tbody>
    {% for uitslag in uitslagen %}
        <tr>
            <td>{{ uitslag.wedstrijd.datum|date('d-m-Y') }}</td>
            <td>{{ uitslag.wedstrijd.naam }}</td>
            <td>{{ uitslag.renner.naam }}</td>
            <td>{% if uitslag.ploeg %}{{ uitslag.ploeg.afkorting }}{% endif %}</td>
            <td>{{ uitslag.ploegPunten }}</td>
            <td>{{ uitslag.rennerPunten }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
<div id="navigation">
    {{ knp_pagination_render(uitslagen) }}
</div>
{% endblock %}