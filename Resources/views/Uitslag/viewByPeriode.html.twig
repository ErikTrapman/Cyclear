{% block content %}
<table class="records_list table table-condensed ">
    <thead>
        <tr>
            <th>Naam</th>
            <th>Punten</th>
        </tr>
    </thead>
    <tbody>
    {% for uitslag in list %}
        <tr>
            <td>{{ uitslag.naam }}</td>
            <td>{{ uitslag.punten }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}