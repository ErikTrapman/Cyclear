{% extends '::admin.base.html.twig' %}

{% block title %}Transfer show{% endblock %}

{% block content %}
<h1>Transfer</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Renner</th>                <td>{{ entity.renner }}</td>        </tr>
        <tr>
            <th>Ploegvan</th>                <td>{{ entity.ploegVan }}</td>        </tr>
        <tr>
            <th>Ploegnaar</th>                <td>{{ entity.ploegNaar }}</td>        </tr>
        <tr>
            <th>Datum</th>                <td>{{ entity.datum|date('Y-m-d H:i:s') }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_transfer') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_transfer_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_transfer_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}