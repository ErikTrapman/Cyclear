{% block content %}
<table class="records_list table table-condensed ">
    <thead>
        <tr>
            <th/>
            <th>IN</th>
            <th>UIT</th>
            <th/>
        </tr>
    </thead>
    <tbody>
    {% for transfer in listWithInversion %}
        <tr>
            <td>{{ transfer.ploegNaar.afkorting }}</td>
            <td>{{ transfer.renner.naam }}</td>
            <td>{% if transfer.inversionTransfer %}{{ transfer.inversionTransfer.renner.naam }}{% endif %}</td>
            <td>{{ transfer.datum|date('d-m-Y H:i') }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}