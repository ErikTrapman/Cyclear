{% block content %}
<table class="records_list table table-condensed ">
    <thead>
        <tr>
            <th/>
            <th>IN</th>
            <th>UIT</th>
            <th/>
        </tr>
    </thead>
    <tbody>
    {% for transfer in listWithInversion %}
        <tr>
            <td>{{ transfer[0].ploegNaar.afkorting }}</td>
            <td>{{ transfer[0].renner.naam }}</td>
            <td>{{ transfer['inverse'] }}</td>
            <td>{{ transfer[0].datum|date('d-m-Y H:i') }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}