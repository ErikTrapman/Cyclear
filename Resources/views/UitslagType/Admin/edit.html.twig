{% extends '::admin.base.html.twig' %}

{% block content %}
<h2>Uitslagtype edit</h2>

<form action="{{ path('admin_uitslagtype_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_uitslagtype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('admin_uitslagtype_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}