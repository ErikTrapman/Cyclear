{% extends '::admin.base.html.twig' %}

{% block content %}
<h1>UitslagType</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Start</th>                <td>{{ entity.start|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Eind</th>                <td>{{ entity.eind|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Transfers</th>                <td>{{ entity.transfers }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_uitslagtype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_uitslagtype_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_uitslagtype_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}