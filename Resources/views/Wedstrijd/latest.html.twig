{% block content %}
<table class="records_list table table-condensed ">
    <thead>
        <tr>
            <th>Datum</th>
            <th>Wedstrijd</th>
        </tr>
    </thead>
    <tbody>
    {% for wedstrijd in wedstrijden %}
        <tr>
            <td nowrap>{{ wedstrijd.datum|date('d-m-Y') }}</td>
            <td>
             {% include "CyclearGameBundle:Wedstrijd:_singleRow.html.twig" with {wedstrijd:wedstrijd, seizoen:wedstrijd.seizoen } %}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}