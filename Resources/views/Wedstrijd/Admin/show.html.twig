{% extends '::admin.base.html.twig' %}

{% block content %}
<h1>Wedstrijd</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Naam</th>                <td>{{ entity.naam }}</td>        </tr>
        <tr>
            <th>Datum</th>                <td>{{ entity.datum|date('Y-m-d H:i:s') }}</td>        
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_wedstrijd') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_wedstrijd_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_wedstrijd_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}