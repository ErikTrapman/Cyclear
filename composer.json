{
    "name": "cyclear/application",
    "description": "A cyclinggame in the Cyclear-tradition",
    "keywords": ["cycling", "game", "cyclinggame","cqranking", "cyclear"],
    "homepage": "http://game.cyclear.nl",
    "autoload": {
        "psr-0": {
            "": "src/",
            "TijsVerkoyen\\Twitter": "vendor/tijsverkoyen/twitteroauth"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "authors": [
        {
            "name": "Erik Trapman",
            "email": "veggatron+cyclear@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",

        "jms/security-extra-bundle": "1.6.*",
        "jms/serializer-bundle": "~1.1.0",

        "kriswallsmith/assetic": "~1.3.2",

        "friendsofsymfony/user-bundle": "dev-master",
        "friendsofsymfony/jsrouting-bundle": "~1.6.0",
        "knplabs/knp-paginator-bundle": "~2.4",
        "knplabs/knp-components": "1.2.*",
        "stof/doctrine-extensions-bundle": "dev-master",

        "liip/functional-test-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/data-fixtures": "dev-master",

        "eriktrapman/cqrankingparser-bundle": "dev-master",
        "beberlei/DoctrineExtensions": "dev-master",
        "suncat/mobile-detect-bundle": "~1.0.3",
        "tijsverkoyen/twitteroauth": "~2.3",
        "ddeboer/data-import": "dev-master",
        "ddeboer/data-import-bundle": "dev-master",
        "samson/dataview-bundle": "1.0.1",
        "twbs/bootstrap": "^3.3",
        "twitter/typeahead.js": "0.11",
        "bmatzner/fontawesome-bundle": "~4.4",
        "vich/uploader-bundle": "~1.3.0",
        "symfony/assetic-bundle": "~2.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                }
            }
        }
    ]
}
