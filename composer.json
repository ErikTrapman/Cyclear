{
    "name": "cyclear/cyclear",
    "description": "A cycling-game in the Cyclear tradition",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "friendsofsymfony/jsrouting-bundle": "^2",
        "friendsofsymfony/user-bundle": "^3",
        "jms/serializer": "^3.18",
        "jms/serializer-bundle": "^5.0",
        "knplabs/knp-components": "1.2.5",
        "knplabs/knp-paginator-bundle": "^2.8.0",
        "samson/dataview-bundle": "1.0.1",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "*",
        "symfony/acl-bundle": "^2.3",
        "symfony/asset": "^4.4",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^2.2",
        "symfony/form": "^4.4",
        "symfony/lock": "4.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^4.4",
        "symfony/property-access": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/templating": "4.4.*",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3",
        "liip/functional-test-bundle": "^4.5",
        "liip/test-fixtures-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*",
        "vimeo/psalm": "^4.29"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "lint-twig": "php bin/console lint:twig --show-deprecations templates/ --ansi",
        "lint-yaml": "php bin/console lint:yaml translations/ config/ --parse-tags",
        "psalm": "psalm --no-cache"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*",
            "docker": false
        }
    }
}
