{
    "name": "cyclear/cyclear",
    "description": "A cycling-game in the Cyclear tradition",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "friendsofsymfony/jsrouting-bundle": "^2",
        "friendsofsymfony/user-bundle": "^3",
        "jms/serializer": "^3.18",
        "jms/serializer-bundle": "^5.0",
        "knplabs/knp-paginator-bundle": "^5",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "*",
        "symfony/acl-bundle": "^2.3",
        "symfony/asset": "^5.4",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/css-selector": "5.4.*",
        "symfony/dom-crawler": "5.4.*",
        "symfony/dotenv": "^5.4.5",
        "symfony/flex": "^2.2",
        "symfony/form": "^5.4",
        "symfony/lock": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^5.4",
        "symfony/property-access": "^5.4",
        "symfony/runtime": "5.4.*",
        "symfony/sendgrid-mailer": "5.4.*",
        "symfony/serializer": "^5.4",
        "symfony/templating": "5.4.*",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^3.4",
        "twig/intl-extra": "^3.4",
        "twig/twig": "^3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "typo3/surf": "dev-upd-monolog as 3.3.5",
        "friendsofphp/php-cs-fixer": "^3",
        "liip/functional-test-bundle": "^4.5",
        "liip/test-fixtures-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^5.4.10",
        "symfony/debug-bundle": "^5.4.10",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "^5.4.10",
        "symfony/web-profiler-bundle": "^5.4.10",
        "vimeo/psalm": "^4.29",
        "vincentlanglet/twig-cs-fixer": "^0.6.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "lint-twig": "php bin/console lint:twig --show-deprecations templates/ --ansi && vendor/bin/twig-cs-fixer lint templates/",
        "lint-twig-fix": "vendor/bin/twig-cs-fixer lint --fix templates/",
        "lint-yaml": "php bin/console lint:yaml translations/ config/ --parse-tags",
        "psalm": "psalm --no-cache",
        "test": "APP_ENV=test vendor/bin/phpunit -c . tests/"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*",
            "docker": false
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:ErikTrapman/Surf.git"
        }
    ]
}
