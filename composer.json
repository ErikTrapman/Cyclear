{
    "name": "cyclear/cyclear",
    "description": "A cycling-game in the Cyclear tradition",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.3",
        "bmatzner/fontawesome-bundle": "^4.7",
        "ddeboer/data-import": "^0.20.0",
        "ddeboer/data-import-bundle": "^0.1.0",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.11",
        "friendsofsymfony/jsrouting-bundle": "^2",
        "friendsofsymfony/user-bundle": "^3",
        "jms/serializer-bundle": "^3",
        "knplabs/knp-components": "1.2.5",
        "knplabs/knp-paginator-bundle": "^2.8.0",
        "samson/dataview-bundle": "1.0.1",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "*",
        "suncat/mobile-detect-bundle": "^1.1",
        "symfony/acl-bundle": "^2.3",
        "symfony/asset": "^4.4",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^2.2",
        "symfony/form": "^4.4",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "^4.4",
        "symfony/property-access": "^4.4",
        "symfony/serializer": "^4.4",
        "symfony/swiftmailer-bundle": "^3",
        "symfony/templating": "4.4.*",
        "symfony/validator": "^4.4",
        "symfony/yaml": "^4.4",
        "tijsverkoyen/twitteroauth": "2.3.1",
        "twbs/bootstrap": "^3.3",
        "twig/twig": "^2",
        "twitter/typeahead.js": "0.11.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3",
        "liip/functional-test-bundle": "^4.5",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-optional-warmers": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@patch-sonata-admin",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@patch-sonata-admin",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*",
            "docker": false
        }
    }
}
